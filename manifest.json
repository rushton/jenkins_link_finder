{
  "manifest_version":2,

  "name": "JenkinsQALinkFinder",
  "description": "Finds failing links in a junit jenkins build",
  "version": "1.0.1",
  "content_scripts": [{
      "matches": ["*://*/*consoleFull"],
      "js":      ["jquery.js","content.js"]
  }],
  "page_action": {
      "default_icon": "icon.png"
  },

  "permissions": ["activeTab"]
}

